	<bean id="gerenteTestSustantivos" class="kalos.datos.gerentes.GerenteTestSustantivosImpl">
		<property name="testSustantivosDAO" ref="testSustantivosDAO" />
	</bean>


	<bean id="gerenteTestVerbos" class="kalos.datos.gerentes.GerenteTestVerbosImpl">
