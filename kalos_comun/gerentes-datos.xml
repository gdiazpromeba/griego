<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
	"http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<!--  transacciones -->
	<bean id="transactionManager" class="org.springframework.jdbc.datasource.DataSourceTransactionManager">
		<property name="dataSource" ref="kalosDataSource" />
	</bean>

	<bean id="transactionAttribute" class="org.springframework.transaction.interceptor.NameMatchTransactionAttributeSource">
		<property name="properties">
			<props>
				<prop key="insertaVarios">PROPAGATION_REQUIRED</prop>
				<prop key="modificaVarios">PROPAGATION_REQUIRED</prop>
				<prop key="ordena">PROPAGATION_REQUIRED</prop>
			</props>
		</property>
	</bean>

	<bean id="transaccionAbstracta" class="org.springframework.transaction.interceptor.TransactionProxyFactoryBean">
		<property name="transactionManager" ref="transactionManager" />
		<property name="transactionAttributeSource" ref="transactionAttribute" />
	</bean>


	<!--  managers -->
	<bean id="gerenteSignificados" class="kalos.datos.gerentes.GerenteSignificadosImpl">
		<property name="significadoDAO" ref="significadoDAO" />
	</bean>

	<bean id="gerenteAdjetivos" parent="transaccionAbstracta">
		<property name="target">
			<bean class="kalos.datos.gerentes.GerenteAdjetivosImpl">
				<property name="adjetivoDAO" ref="adjetivoDAO" />
				<property name="irrAdjetivosEnterosDAO" ref="irrAdjetivosEnterosDAO" />
				<property name="irrAdjetivosIndividualesDAO" ref="irrAdjetivosIndividualesDAO" />
				<property name="gerenteSignificados" ref="gerenteSignificados" />
				<property name="gerenteAdjetivosComoNominales" ref="gerenteAdjetivosComoNominales" />
				<property name="gerenteIrrAdjetivosEnteros" ref="gerenteIrrAdjetivosEnteros" />
			</bean>
		</property>
	</bean>
	
	<bean id="gerenteAdverbios" parent="transaccionAbstracta">
		<property name="target">
			<bean class="kalos.datos.gerentes.GerenteAdverbiosImpl">
				<property name="adverbiosDAO" ref="adverbiosDAO" />
				<property name="gerenteSignificados" ref="gerenteSignificados" />
			</bean>
		</property>
	</bean>
	
	<bean id="gerenteInterjecciones" parent="transaccionAbstracta">
		<property name="target">
			<bean class="kalos.datos.gerentes.GerenteInterjeccionesImpl">
				<property name="interjeccionesDAO" ref="interjeccionesDAO" />
				<property name="gerenteSignificados" ref="gerenteSignificados" />
			</bean>
		</property>
	</bean>	
		

	<bean id="gerenteAdjetivosComoNominales" parent="transaccionAbstracta">
		<property name="target">
			<bean class="kalos.datos.gerentes.GerenteAdjetivosComoNominalesImpl">
				<property name="adjetivosComoNominalesDAO" ref="adjetivosComoNominalesDAO" />
			</bean>
		</property>
	</bean>


	<bean id="gerenteCombosValores" class="kalos.datos.gerentes.GerenteCombosValoresImpl">
		<property name="combosDAO" ref="combosDAO" />
	</bean>
	
	<bean id="gerenteConjunciones" class="kalos.datos.gerentes.GerenteConjuncionesImpl">
		<property name="conjuncionesDAO" ref="conjuncionesDAO" />
		<property name="gerenteSignificados" ref="gerenteSignificados" />
	</bean>	

	<bean id="gerenteCubosTipoPart" class="kalos.datos.gerentes.GerenteCubosTipoPartImpl" lazy-init="true">
		<property name="cubosTipoPartDAO" ref="cubosTipoPartDAO" />
	</bean>


	<bean id="gerenteDesinSust" class="kalos.datos.gerentes.GerenteDesinSustImpl">
		<property name="desinSustDAO" ref="desinSustDAO" />
	</bean>
	
	<bean id="gerenteDesinInfinitivos" class="kalos.datos.gerentes.GerenteDesinInfinitivosImpl">
		<property name="desinInfinitivosDAO" ref="desinInfinitivosDAO" />
	</bean>

	<bean id="gerenteDesinVerbos" class="kalos.datos.gerentes.GerenteDesinVerbosImpl">
		<property name="desinVerbosDAO" ref="desinVerbosDAO" />
	</bean>
	
    <bean id="gerenteEncParticulas" class="kalos.datos.gerentes.GerenteEncParticulasImpl">
    		<property name="encParticulasDAO" ref="encParticulasDAO" />
	</bean>
    
	
	<bean id="gerenteDiccionario" class="kalos.datos.gerentes.GerenteDiccionarioImpl">
	  <property name="diccionarioDAO" ref="diccionarioDAO" />
	</bean>		

	<bean id="gerenteIrrAdjetivosEnteros" class="kalos.datos.gerentes.GerenteIrrAdjetivosEnterosImpl">
		<property name="irrAdjetivosEnterosDAO" ref="irrAdjetivosEnterosDAO" />
	</bean>
	
	<bean id="gerenteIrrAdjetivosIndividuales" class="kalos.datos.gerentes.GerenteIrrAdjetivosIndividualesImpl">
		<property name="irrAdjetivosIndividualesDAO" ref="irrAdjetivosIndividualesDAO" />
