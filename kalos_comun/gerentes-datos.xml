	</bean>


	<bean id="gerenteTiposSustantivo" class="kalos.datos.gerentes.GerenteTiposSustantivoImpl">
		<property name="tiposSustantivoDAO" ref="tiposSustantivoDAO" />
	</bean>

	<bean id="gerenteTiposVerbo" class="kalos.datos.gerentes.GerenteTiposVerboImpl">
		<property name="tiposVerboDAO" ref="tiposVerboDAO" />
	</bean>


	<bean id="gerenteTermRegInfinitivo" class="kalos.datos.gerentes.GerenteTermRegInfinitivoImpl">
		<property name="termRegInfinitivoDAO" ref="termRegInfinitivoDAO" />
	</bean>

	<bean id="gerenteTermRegSustantivo" class="kalos.datos.gerentes.GerenteTermRegSustantivoImpl">
