	</bean>


	<bean id="gerenteVerbos" parent="transaccionAbstracta" lazy-init="true">
		<property name="target">
			<bean class="kalos.datos.gerentes.GerenteVerbosImpl">
			    <property  name="verbosDAO" ref="verbosDAO" />
				<property name="irrVerbosDAO" ref="irrVerbosDAO" />
				<property name="irrVerbosIndividualesDAO" ref="irrVerbosIndividualesDAO" />
				<property name="gerenteSignificados" ref="gerenteSignificados" />
				<property name="gerentePreposicionesEnVerbos" ref="gerentePreposicionesEnVerbos" />
				<property name="gerenteVerbosCompuestos" ref="gerenteVerbosCompuestos" />
			</bean>
		</property>
	</bean>
	
	<bean id="gerenteVerbosCompuestos" class="kalos.datos.gerentes.GerenteVerbosCompuestosImpl" lazy-init="true">
		<property name="verbosCompuestosDAO" ref="verbosCompuestosDAO" />
	    <property name="preposicionesEnVerbosDAO" ref="preposicionesEnVerbosDAO" />		
	</bean>

