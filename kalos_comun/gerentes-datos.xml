	<bean id="gerenteIrrVerbos" class="kalos.datos.gerentes.GerenteIrrVerbosImpl">
		<property name="irrVerbosDAO" ref="irrVerbosDAO" />
	</bean>

	<bean id="gerenteIrrVerbosIndividuales" class="kalos.datos.gerentes.GerenteIrrVerbosIndividualesImpl">
		<property name="irrVerbosIndividualesDAO" ref="irrVerbosIndividualesDAO" />
	</bean>

	<bean id="gerenteTestAdjetivos" class="kalos.datos.gerentes.GerenteTestAdjetivosImpl">
		<property name="testAdjetivosDAO" ref="testAdjetivosDAO" />
	</bean>

	<bean id="gerenteTiposAdjetivo" class="kalos.datos.gerentes.GerenteTiposAdjetivoImpl">
		<property name="tiposAdjetivoDAO" ref="tiposAdjetivoDAO" />
	</bean>
	
	<bean id="gerenteParticulas" class="kalos.datos.gerentes.GerenteParticulasImpl">
		<property name="particulasDAO" ref="particulasDAO" />
		<property name="gerenteSignificados" ref="gerenteSignificados" />		
	</bean>	
	
