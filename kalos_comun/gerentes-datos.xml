	<bean id="gerenteIrrParticipiosSimples" class="kalos.datos.gerentes.GerenteIrrParticipiosSimplesImpl" lazy-init="true">
		<property name="irrParticipiosSimplesDAO" ref="irrParticipiosSimplesDAO" />
