	</bean>

	<bean id="gerenteIrrParticipiosEnteros" class="kalos.datos.gerentes.GerenteIrrParticipiosEnterosImpl" lazy-init="true">
		<property name="irrParticipiosEnterosDAO" ref="irrParticipiosEnterosDAO" />
