		<property name="gerenteIrrVerbosIndividuales" ref="gerenteIrrVerbosIndividuales" />
	</bean>


	<bean id="amInfinitivos" class="com.kalos.analisismorfologico.negocio.AMInfinitivos" lazy-init="true">
		<property name="amUtil" ref="amUtil" />
		<property name="amVerbal" ref="amVerbal" />
		<property name="extractorPrefijos" ref="extractorPrefijos" />
		<property name="gerenteTermRegInfinitivo" ref="gerenteTermRegInfinitivo" />
		<property name="gerenteVerbos" ref="gerenteVerbos" />
		<property name="gerenteIrrInfinitivos" ref="gerenteIrrInfinitivos" />
	</bean>
	

	
	<bean id="amNominal" class="com.kalos.analisismorfologico.negocio.AMNominal" lazy-init="true">
		<property name="amUtil" ref="amUtil" />
		<property name="gerenteTermRegSustantivo" ref="gerenteTermRegSustantivo" />
		<constructor-arg ref="gerenteTiposSustantivo"/>
		<constructor-arg ref="gerenteDesinSust"/>
	</bean>

