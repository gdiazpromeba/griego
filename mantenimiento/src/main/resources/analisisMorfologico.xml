		<property name="amUtil" ref="amUtil" />
		<property name="amVerbal" ref="amVerbal" />
		<property name="amNominal" ref="amNominal" />
		<property name="participios" ref="participios" />
		<property name="extractorPrefijos" ref="extractorPrefijos" />
		<property name="gerenteVerbos" ref="gerenteVerbos" />
        <property name="gerenteVerbalizadorParticipios" ref="gerenteVerbalizadorParticipios" />
        <property name="gerenteVerbosCompuestos" ref="gerenteVerbosCompuestos" />        
        -->
