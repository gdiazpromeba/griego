<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
	"http://www.springframework.org/dtd/spring-beans.dtd">
<beans>


	<bean id="cacheFlexionParticipio" class="com.kalos.flexion.declinacion.CacheFlexionParticipio" />
	<bean id="cacheFlexionVerbos" class="com.kalos.flexion.conjugacion.CacheFlexionVerbos" />
	<bean id="cacheFlexionInfinitivos" class="com.kalos.flexion.conjugacion.CacheFlexionInfinitivos" />

	<bean id="creadorTableModelsBasicos" class="com.kalos.flexion.CreadorTableModelsBasicos">
		<property name="utilidadesTM" ref="utilidadesTM" />
		<property name="proveedorDMNominal" ref="proveedorDMNominal" />
		<property name="proveedorDMVerbal" ref="proveedorDMVerbal" />
		<property name="proveedorDMParticulas" ref="proveedorDMParticulas" />
		<property name="proveedorDMConjunciones" ref="proveedorDMConjunciones" />
		<property name="proveedorDMPreposiciones" ref="proveedorDMPreposiciones" />
		<property name="proveedorDMInterjecciones" ref="proveedorDMInterjecciones" />
	</bean>

	<bean id="creadorTableModelsReporte" class="com.kalos.flexion.CreadorTableModelsReporte">
		<property name="creadorTableModelsBasicos" ref="creadorTableModelsBasicos" />
		<property name="utilidadesTM" ref="utilidadesTM" />
	</bean>

	<bean id="creadorTableModelsGrilla" class="com.kalos.flexion.CreadorTableModelsGrilla">
		<property name="creadorTableModelsBasicos" ref="creadorTableModelsBasicos" />
		<property name="utilidadesTM" ref="utilidadesTM" />
		<property name="utilidadReportes" ref="utilidadReportes" />
	</bean>




	<bean id="declina" class="com.kalos.flexion.declinacion.Declina">
		<property name="manejaDesinenciasSustantivo" ref="manejaDesinenciasSustantivo" />
		<property name="gerenteTiposSustantivo" ref="gerenteTiposSustantivo" />
		<property name="gerenteSustantivos" ref="gerenteSustantivos" />
		<property name="gerenteIrrSustantivos" ref="gerenteIrrSustantivos" />
	</bean>

	<bean id="declinaAdjetivos" class="com.kalos.flexion.declinacion.DeclinaAdjetivos">
		<property name="declina" ref="declina" />
		<property name="gerenteTiposAdjetivo" ref="gerenteTiposAdjetivo" />
		<property name="gerenteTiposSustantivo" ref="gerenteTiposSustantivo" />
		<property name="gerenteAdjetivos" ref="gerenteAdjetivos" />
		<property name="gerenteIrrAdjetivosEnteros" ref="gerenteIrrAdjetivosEnteros" />
		<property name="gerenteIrrAdjetivosIndividuales" ref="gerenteIrrAdjetivosIndividuales" />
	</bean>

	<bean id="infinitivos" class="com.kalos.flexion.conjugacion.Infinitivos">
		<property name="verbos" ref="verbos" />
		<property name="gerenteVerbos" ref="gerenteVerbos" />
		<property name="gerenteIrrVerbos" ref="gerenteIrrVerbos" />
		<property name="gerenteIrrInfinitivos" ref="gerenteIrrInfinitivos" />
		<property name="manejaDesinenciasInfinitivo" ref="manejaDesinenciasInfinitivo" />
		<property name="preposiciones" ref="preposiciones" />
		<property name="gerenteVerbosCompuestos" ref="gerenteVerbosCompuestos" />
		<property name="gerentePreposicionesEnVerbos" ref="gerentePreposicionesEnVerbos" />
	</bean>


	<bean id="manejaDesinenciasSustantivo" class="com.kalos.flexion.declinacion.ManejaDesinenciasSustantivo">
		<property name="gerenteTiposSustantivo" ref="gerenteTiposSustantivo" />
		<property name="gerenteDesinSust" ref="gerenteDesinSust" />
	</bean>

	<bean id="manejaDesinenciasVerbo" class="com.kalos.flexion.conjugacion.negocio.ManejaDesinenciasVerbo">
		<constructor-arg value="false" />
		<constructor-arg ref="gerenteDesinVerbos" />
	</bean>

	<bean id="manejaDesinenciasInfinitivo" class="com.kalos.flexion.conjugacion.negocio.ManejaDesinenciasInfinitivo">
		<constructor-arg value="false" />
		<constructor-arg ref="gerenteDesinInfinitivos" />
	</bean>

	<bean id="participios" class="com.kalos.flexion.declinacion.Participios" lazy-init="true">
		<property name="declina" ref="declina" />
		<property name="gerenteVerbos" ref="gerenteVerbos" />
		<property name="preposiciones" ref="preposiciones" />		
		<property name="cacheFlexionParticipio" ref="cacheFlexionParticipio" />
		<property name="gerenteVerbosCompuestos" ref="gerenteVerbosCompuestos" />
		<property name="gerentePreposicionesEnVerbos" ref="gerentePreposicionesEnVerbos" />
		<property name="gerenteIrrVerbos" ref="gerenteIrrVerbos" />
		<property name="gerenteIrrParticipiosEnteros" ref="gerenteIrrParticipiosEnteros" />
		<property name="gerenteIrrParticipiosSimples" ref="gerenteIrrParticipiosSimples" />
	</bean>




	<bean id="preposiciones" class="com.kalos.flexion.Preposiciones" />

	<bean id="proveedorDMNominal" class="com.kalos.flexion.ProveedorDMNominal">
		<property name="participios" ref="participios" />
		<property name="gerenteVerbos" ref="gerenteVerbos" />
		<property name="declina" ref="declina" />
		<property name="declinaAdjetivos" ref="declinaAdjetivos" />
		<property name="utilidadesTM" ref="utilidadesTM" />
	</bean>
	
    <bean id="proveedorDMConjunciones" class="com.kalos.flexion.ProveedorDMConjunciones">
		<property name="gerenteConjunciones" ref="gerenteConjunciones" />
	</bean>

	<bean id="proveedorDMParticulas" class="com.kalos.flexion.ProveedorDMParticulas">
		<property name="gerenteParticulas" ref="gerenteParticulas" />
	</bean>
	
	<bean id="proveedorDMPreposiciones" class="com.kalos.flexion.ProveedorDMPreposiciones">
		<property name="gerentePreposiciones" ref="gerentePreposiciones" />
	</bean>	
	
	<bean id="proveedorDMInterjecciones" class="com.kalos.flexion.ProveedorDMInterjecciones">
		<property name="gerenteInterjecciones" ref="gerenteInterjecciones" />
	</bean>	
		

	<bean id="proveedorDMVerbal" class="com.kalos.flexion.ProveedorDMVerbal">
		<property name="gerenteIrrVerbos" ref="gerenteIrrVerbos" />
		<property name="gerenteVerbos" ref="gerenteVerbos" />
		<property name="verbos" ref="verbos" />
		<property name="infinitivos" ref="infinitivos" />
		<property name="utilidadesTM" ref="utilidadesTM" />
		<property name="cacheFlexionVerbos" ref="cacheFlexionVerbos" />		
	    <property name="cacheFlexionInfinitivos" ref="cacheFlexionInfinitivos" />		
	</bean>


	<bean id="utilidadReportes" class="com.kalos.flexion.UtilidadReportes"/>
	<bean id="utilidadesTM" class="com.kalos.flexion.UtilidadesTM" />



	<bean id="verbos" class="com.kalos.flexion.conjugacion.Verbos">
		<property name="manejaDesinenciasVerbo" ref="manejaDesinenciasVerbo" />
		<property name="gerenteTiposVerbo" ref="gerenteTiposVerbo" />
		<property name="gerenteIrrVerbos" ref="gerenteIrrVerbos" />
		<property name="cacheFlexionVerbos" ref="cacheFlexionVerbos" />
		<property name="gerenteIrrVerbosIndividuales" ref="gerenteIrrVerbosIndividuales" />
		<property name="gerenteVerbos" ref="gerenteVerbos" />
		<property name="preposiciones" ref="preposiciones" />
		<property name="gerentePreposicionesEnVerbos" ref="gerentePreposicionesEnVerbos" />
		<property name="gerenteVerbosCompuestos" ref="gerenteVerbosCompuestos" />
	</bean>



</beans>
