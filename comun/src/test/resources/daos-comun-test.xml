<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
     xmlns:context="http://www.springframework.org/schema/context"
     xsi:schemaLocation="http://www.springframework.org/schema/beans
         http://www.springframework.org/schema/beans/spring-beans.xsd
         http://www.springframework.org/schema/context
         http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config/> 
    

	<bean id="testAdjetivosDAO" class="com.kalos.datos.dao.TestAdjetivosDAOImpl">
		<property name="dataSource" ref="kalosDataSource" />
	</bean>

	<bean id="testInfinitivosDAO" class="com.kalos.datos.dao.TestInfinitivosDAOImpl">
		<property name="dataSource" ref="kalosDataSource" />
	</bean>

	<bean id="testParticipiosDAO" class="com.kalos.datos.dao.TestParticipiosDAOImpl">
		<property name="dataSource" ref="kalosDataSource" />
	</bean>

	<bean id="testSustantivosDAO" class="com.kalos.datos.dao.TestSustantivosDAOImpl">
		<property name="dataSource" ref="kalosDataSource" />
	</bean>


	<bean id="testVerbosDAO" class="com.kalos.datos.dao.TestVerbosDAOImpl">
		<property name="dataSource" ref="kalosDataSource" />
	</bean>




</beans>
